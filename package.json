{
  "name": "ldclient-js",
  "version": "1.4.0-beta",
  "description": "LaunchDarkly SDK for JavaScript",
  "author": "LaunchDarkly <team@launchdarkly.com>",
  "license": "Apache-2.0",
  "keywords": [
    "launchdarkly",
    "analytics",
    "client"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/ldclient.cjs.js",
  "module": "dist/@ldclient.es.js",
  "browser": "dist/@ldclient.umd.js",
  "scripts": {
    "preversion": "npm run build",
    "prebuild": "rimraf dist/**",
    "lint": "eslint --format 'node_modules/eslint-formatter-pretty' src",
    "format": "npm run format:md && npm run format:js",
    "format:md": "prettier --parser markdown --write '*.md'",
    "format:js": "prettier --write 'src/**/*.js'",
    "build": "cross-env npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:cjs",
    "build:watch": "cross-env npm run build:es -- -w",
    "build:umd": "cross-env NODE_ENV=development rollup -c rollup.config.js -f umd -i src/index.js -o dist/ldclient.umd.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c rollup.config.js -f umd -i src/index.js -o dist/ldclient.umd.min.js",
    "build:es": "cross-env NODE_ENV=development rollup -c rollup.config.js -f es -i src/index.js -o dist/ldclient.es.js",
    "build:cjs": "cross-env NODE_ENV=development rollup -c rollup.config.js -f cjs -i src/index.js -o dist/ldclient.cjs.js",
    "test": "karma start --log-level debug",
    "test:watch": "karma start --auto-watch --reporters=dots"
  },
  "types": "./src/index.d.ts",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "4.1.2",
    "eslint-plugin-chai-friendly": "0.4.1",
    "cross-env": "^5.1.3",
    "karma": "2.0.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-rollup-preprocessor": "5.0.2",
    "karma-phantomjs-shim": "1.5.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "mocha": "4.0.1",
    "phantomjs": "2.1.7",
    "prettier": "^1.9.2",
    "readline-sync": "1.4.7",
    "semver-compare": "^1.0.0",
    "sinon": "1.17.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-commonjs": "8.2.3",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-mocha": "^4.11.0 "
  },
  "dependencies": {
    "Base64": "1.0.1",
    "escape-string-regexp": "1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/launchdarkly/js-client.git"
  }
}
