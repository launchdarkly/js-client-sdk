{
  "name": "ldclient-js",
  "version": "2.0.0",
  "description": "LaunchDarkly SDK for JavaScript",
  "author": "LaunchDarkly <team@launchdarkly.com>",
  "license": "Apache-2.0",
  "keywords": [
    "launchdarkly",
    "analytics",
    "client"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/ldclient.cjs.js",
  "module": "dist/ldclient.es.js",
  "scripts": {
    "lint": "eslint --format 'node_modules/eslint-formatter-pretty'",
    "lint:all": "eslint --format 'node_modules/eslint-formatter-pretty' src",
    "format": "npm run format:md && npm run format:js",
    "format:md": "prettier --parser markdown --write '*.md'",
    "format:js": "prettier --write 'src/**/*.js'",
    "format:test": "npm run format:test:md && npm run format:test:js",
    "format:test:md": "prettier --parser markdown --list-different '*.md'",
    "format:test:js": "prettier --list-different 'src/**/*.js'",
    "build": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "build:min": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:watch": "cross-env npm run build -- -w",
    "test": "NODE_ENV=test jest",
    "test:junit": "NODE_ENV=test jest --testResultsProcessor jest-junit",
    "clean": "rimraf dist/**",
    "prepublishOnly": "npm run build:min"
  },
  "types": "./src/index.d.ts",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-1": "6.24.1",
    "chai": "4.1.2",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-xo": "0.20.1",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-plugin-babel": "5.0.0",
    "eslint-plugin-prettier": "2.6.0",
    "eventsourcemock": "1.1.0",
    "jest": "22.4.3",
    "jest-junit": "3.6.0",
    "jest-localstorage-mock": "2.2.0",
    "prettier": "1.11.1",
    "readline-sync": "1.4.9",
    "rimraf": "2.6.2",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-includepaths": "0.2.2",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.2.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-watch": "4.3.1",
    "semver": "5.5.0",
    "semver-compare": "1.0.0",
    "sinon": "4.5.0"
  },
  "dependencies": {
    "Base64": "1.0.1",
    "escape-string-regexp": "1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/launchdarkly/js-client.git"
  }
}
